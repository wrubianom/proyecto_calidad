            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>ELIMINAR_USUARIO</from-outcome>
            <to-view-id>/pages/EliminarUsuario.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>ADICIONAR_MATERIA</from-outcome>
            <to-view-id>/pages/adicionarMateria.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>ELIMINAR_MATERIA</from-outcome>
            <to-view-id>/pages/EliminarMateriaF.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>MODIFICAR_MATERIA</from-outcome>
            <to-view-id>/pages/ModificarMateria.xhtml</to-view-id>
            <redirect/>
