<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>HOME</from-outcome>
            <to-view-id>/pages/home.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>HOME</from-outcome>
            <to-view-id>/pages/home.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>LOGIN</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/LoginFail.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>HOME</from-outcome>
            <to-view-id>/pages/home.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/LoginFail.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>LOGIN_FAIL</from-outcome>
            <to-view-id>/LoginFail.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/*</from-view-id>
        <navigation-case>
            <from-outcome>HOME</from-outcome>
            <to-view-id>/pages/home.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>ADICIONAR_GRUPO</from-outcome>
            <to-view-id>/pages/AdicionarGrupo.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>ELIMINAR_GRUPO</from-outcome>
            <to-view-id>/pages/EliminarGrupo.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>ASIGNAR_GRUPO</from-outcome>
            <to-view-id>/pages/AsignarGrupo.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>CONSULTAR_MATERIAS_PROFESOR</from-outcome>
            <to-view-id>/pages/ConsultarMateriasProfesor.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>ADICIONAR_PROFESOR</from-outcome>
            <to-view-id>/pages/AdicionarProfesor.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>CONSULTAR_PROFESOR</from-outcome>
            <to-view-id>/pages/ConsultarProfesor.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>MODIFICAR_PROFESOR</from-outcome>
            <to-view-id>/pages/ModificarProfesor.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>ELIMINAR_PROFESOR</from-outcome>
            <to-view-id>/pages/EliminarProfesor.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>ADICIONAR_USUARIO</from-outcome>
            <to-view-id>/pages/AdicionarUsuario.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>CONSULTAR_USUARIO</from-outcome>
            <to-view-id>/pages/ConsultarUsuario.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
